	sar_squint	90;
	sar_spotlight_x	2400;
	sar_spotlight_y	2400;
	sar_map_size	1000;
	sar_mode	0;
	sar_resolution_option	0;
	window	Blank;
	observer	visual;
	enable	on;
	output	1;
	disp_raster_base_color	0.501961	0.501961	0.501961;
	alt_offset	0;
